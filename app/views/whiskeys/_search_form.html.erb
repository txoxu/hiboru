<%= form_with model: @search_form, url: whiskeys_path, method: :get, local: true do |f| %>
  <%= f.label :category_names, '国別' %>
  <%= f.select :category_names, options_for_select(@category_names.map(&:category_name)), include_blank: true %>

  <%= f.label :category_types, '原料別' %>
  <%= f.select :category_types, options_for_select(@category_types.map(&:category_type)), include_blank: true %>

  <%= f.label :name, '名前' %>
  <%= f.text_field :name %>

  <%= f.label :text, '説明' %>
  <%= f.text_field :text %>

  <%= f.submit '検索' %>
<% end %>
